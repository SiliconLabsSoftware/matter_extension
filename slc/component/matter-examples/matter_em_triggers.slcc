category: Matter|Examples
package: matter
description: >
  Provides example component that enables support for the Energy Management Triggers device type.
id: matter_em_triggers
quality: production
metadata:
  sbom:
    license: MSLA
requires:
  - name: matter_dem
  - name: matter_electrical_sensor
provides:
  - name: matter_em_triggers
include:
  - path: third_party/matter_sdk/examples/energy-management/energy-management-triggers/
    file_list:
      - path: FakeReadings.h

source:
  - path: third_party/matter_sdk/examples/energy-management/energy-management-triggers/FakeReadings.cpp
  - path: third_party/matter_sdk/examples/energy-management/energy-management-triggers/EnergyReportingEventTriggers.cpp
  - path: third_party/matter_sdk/examples/energy-management/energy-management-triggers/DEMTestEventTriggers.cpp
ui_hints:
  visibility: never