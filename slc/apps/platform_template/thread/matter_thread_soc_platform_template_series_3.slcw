workspace_name: matter_thread_soc_platform_template_series_3
label: Matter - SoC Platform Template over Thread with internal Bootloader
quality: production
package: matter
description: Demonstrates a sample implementation of a Matter over Thread platform template app with internal bootloader
project:
  - path: ./matter_thread_soc_platform_template.slcp
    id: application
    output: platform_template
  - path: ../../bootloaders/bootloader-storage-single-4096k-series-3/matter_bootloader_internal_series_3.slcp
    id: bootloader
    output: matter_bootloader
post_build:
  path: ../../../postbuild/ws_default_application_bootloader.slpb
